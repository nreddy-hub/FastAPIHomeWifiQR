# Environment variables for Docker Compose
# Copy this file to .env and update with your values

# Database Configuration
SQL_PASSWORD=YourStrong@Passw0rd

# JWT Configuration
JWT_SIGNING_KEY=your-secret-signing-key-change-in-production-must-be-at-least-32-characters-long

# AWS Configuration (Optional - for SQS/Lambda)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR-ACCOUNT-ID/wifi-qr-queue
ENABLE_SQS=false

# Production Database (for docker-compose.prod.yml)
DATABASE_CONNECTION_STRING=Server=your-db-server;Database=HomeWifiQRDb;User Id=sa;Password=YourPassword;TrustServerCertificate=True;
